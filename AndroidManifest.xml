<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ghsoft.treetask"
    android:versionCode="3"
    android:versionName="1.2" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher_icon"
        android:label="@string/app_name"
        android:theme="@style/Theme.Treetask" >
        <activity android:name="com.ghsoft.treetaskapp.Main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ghsoft.treetaskapp.TaskView" >
        </activity>
        <activity android:name="com.ghsoft.treetaskapp.NewTaskTree" >
        </activity>
        <activity android:name="com.ghsoft.treetaskapp.NewTask" >
        </activity>
        <activity android:name="com.ghsoft.treetaskapp.EditTask" >
        </activity>
        <activity android:name="com.ghsoft.treetaskapp.TreeView" >
        </activity>
        <activity android:name="com.ghsoft.treetaskapp.ExportView" >
        </activity>
    </application>

</manifest>